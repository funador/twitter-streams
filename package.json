{
  "name": "twitter-streams",
  "version": "0.0.11",
  "description": "Pulling quality content from Twitter",
  "main": "lib/server.js",
  "scripts": {
    "serve": "nodemon lib/twitter.js --exec babel-node --presets es2015,stage-2",
    "build": "babel lib -d dist --presets es2015,stage-2 && regenerator --include-runtime dist/twitter.js > dist/twitterES6.js",
    "start": "forever dist/twitterES6.js"
  },
  "author": "Jesse Heaslip <jesse.heaslip@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^2.0.0",
    "expand-url": "^0.1.3",
    "express": "^4.14.0",
    "firebase": "^3.6.0",
    "forever": "^0.15.3",
    "nodemon": "^1.10.0",
    "request": "^2.78.0",
    "request-image-size": "^1.2.2",
    "twit": "^2.2.5",
    "unfluff": "^1.1.0",
    "url-expand": "^0.1.5"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.11.6"
  }
}
